{
  "version": 2,
  "builds": [
    {
      "src": "./server/src/*.ts",
      "use": "@vercel/node",
      "config": {
        "distDir": "./server/build/"
      }
    },
    {
      "src": "./client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "./client/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./server/build/"
    }
  ]
}